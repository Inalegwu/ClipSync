		"react-dom": "^18.2.0",
		"react-hot-toast": "^2.4.0",
		"react-icons": "^4.8.0",
		"react-router-dom": "^6.10.0",
		"sort-by": "^1.2.0",
		"superjson": "^1.12.3",
		"zustand": "^4.3.7"
	},
	"devDependencies": {
		"@types/node": "^18.15.13",
		"@types/react": "^18.0.38",
		"@types/react-dom": "^18.0.11",
		"cross-env": "^7.0.3",
		"css-loader": "^6.7.3",
		"electron": "^24.1.2",
		"electronmon": "^2.0.2",
		"html-webpack-plugin": "^5.5.1",
